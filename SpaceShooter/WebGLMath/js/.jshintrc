    "Sampler2D" : true,
    "SamplerCube" : true,
    "VecArray" : true,
    "Vec1Array" : true,
    "Vec2Array" : true,
    "Vec3Array" : true,
    "Vec4Array" : true,
    "Mat4Array" : true,
    "Sampler2DArray" : true,
    "SamplerCubeArray" : true,
