Shader.source[document.currentScript.src.split('js/shaders/')[1]] = `#version 300 es

  uniform struct {
    mat4 modelMatrix;
  } uniformModelMatrix;

  //uniform mat4 modelMatrix;

  in vec4 vertexPosition;
  in vec4 vertexTexCoord;

  out vec2 tex;

  void main(void) {
    gl_Position = vec4(vertexPosition.x * 0.1f, vertexPosition.y * 0.1f, 0, 1 ) * uniformModelMatrix.modelMatrix ;
    tex.x = vertexTexCoord.x;
    tex.y = vertexTexCoord.y;
  }
`;